{
    "name": "Tab Manager",
    "version": "0.4",
    "description": "Manage your tabs and get work done",
    "manifest_version": 2,
    "permissions": ["storage", "tabs"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "images/*.png",
        "sessions.html"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}